{
  "name": "the-jack-one",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "webpack",
    "serve": "webpack-dev-server",
    "test": "nyc mocha --opts .mocha",
    "watch": "nyc mocha --opts .mocha --watch",
    "start": "npm run serve",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage:codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "typedoc --out dist/docs --module commonjs src/Engine",
    "predeploy": "npm run docs && npm run build",
    "deploy": "gh-pages -d dist",
    "prepush": "npm run test"
  },
  "author": "yujuiting <yujuiting@me.com> (https://github.com/yujuiting)",
  "repository": {
    "type": "git",
    "url": "https://github.com/yujuiting/the-jack-one.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/copy-webpack-plugin": "^4.0.0",
    "@types/glob": "^5.0.30",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jsdom": "^11.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.8",
    "@types/sinon": "^2.3.3",
    "@types/sinon-chai": "^2.7.28",
    "@types/webpack": "^3.0.9",
    "awesome-typescript-loader": "^3.2.1",
    "canvas": "^1.6.5",
    "chai": "^4.0.2",
    "codecov": "^2.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.7",
    "nyc": "^11.0.3",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.12.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.2.0",
    "tsconfig-paths": "^2.2.0",
    "tslint": "^5.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-microsoft-contrib": "^5.0.1",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2"
  },
  "nyc": {
    "check-coverage": false,
    "include": [
      "src/Engine/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  }
}
